{
  "name": "@babsey/code-graph",
  "version": "0.3.0",
  "author": "babsey <spreizer@web.de>",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "main": "./dist/code-mirror.umd.cjs",
  "module": "./dist/code-graph.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/code-graph.js",
      "require": "./dist/code-graph.umd.cjs"
    },
    "./style.css": "./dist/code-graph.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p clean type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "clean": "rimraf dist",
    "lint": "eslint . --fix",
    "format": "prettier --write examples/**/src/ lib/ src/"
  },
  "dependencies": {
    "@codemirror/lang-python": "~6.2",
    "@codemirror/theme-one-dark": "~6.1",
    "@vueuse/core": "~14.0",
    "baklavajs": "~2.8",
    "codemirror": "~6.0",
    "mustache": "~4.2",
    "splitpanes": "~4.0",
    "toposort": "~2.0",
    "uuid": "~13.0",
    "vue": "~3.5",
    "vue-clipboard3": "~2.0",
    "vue-codemirror": "~6.1"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "~22.0",
    "@types/mustache": "~4.2",
    "@types/node": "~24.10",
    "@types/toposort": "~2.0",
    "@vitejs/plugin-vue": "~6.0",
    "@vue/eslint-config-prettier": "~10.2",
    "@vue/eslint-config-typescript": "~14.6",
    "@vue/language-core": "~3.1",
    "@vue/tsconfig": "~0.8",
    "eslint": "~9.39",
    "eslint-plugin-vue": "~10.5",
    "jiti": "~2.6",
    "npm-run-all2": "~8.0",
    "prettier": "~3.6",
    "rimraf": "~6.1",
    "sass-embedded": "~1.93",
    "typescript": "~5.9",
    "vite": "~7.1",
    "vite-plugin-vue-devtools": "~8.0",
    "vue-tsc": "~3.1"
  }
}
